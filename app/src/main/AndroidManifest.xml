<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gestionserverminecraft">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:permissionGroup="android.permission-group.STORAGE"
        android:protectionLevel="dangerous" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/minecraftlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/minecraftlogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Creditos" />
        <activity android:name=".DetallActivity" />
        <activity android:name=".Lista_Crafteo_Detalles"></activity>
        <activity android:name=".llista_nom_crafteo" />
        <activity android:name=".llista_nom_mod" />
        <activity android:name=".Llista_Crafteos_Item" />
        <activity android:name=".Lista_Crafteos" />
        <activity android:name=".Llista_mods_item" />
        <activity android:name=".Delete_Crafteos" />
        <activity android:name=".Delete_Mods" />
        <activity android:name=".Update_Crafteos" />
        <activity android:name=".Update_Mods" />
        <activity android:name=".Insert_Crafteos" />
        <activity android:name=".Insert_Mods" />
        <activity android:name=".Lista_Mods" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>